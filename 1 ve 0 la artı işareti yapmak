#include<iostream>

using namespace std;
int main(){
int number;

cin>>number;

for(int i=0;i<number;i++){
    for(int j=0;j<number;j++){
      if(i==number/2||j==number/2||(number%2==0&&(i==number/2-1||j==number/2-1)))
      cout<<"1";
      else{
        cout<<"0";
      }
    }
    cout<<endl;
 }
}

